{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "objects",
  "definitions": {
    "owner_state": {
      "type": "object",
      "properties": {
        "state": {
          "type": "integer",
          "enum": [
            1,
            2,
            3,
            4,
            5
          ],
          "enumNames": [
            "banned",
            "adult",
            "hidden",
            "deleted",
            "blacklisted"
          ]
        },
        "description": {
          "type": "string",
          "description": "wiki text to describe user state"
        }
      },
      "additionalProperties": false
    }
  }
}
